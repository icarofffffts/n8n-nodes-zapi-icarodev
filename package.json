{
  "name": "n8n-nodes-zapi-icarodev",
  "version": "1.0.0",
  "description": "Node de comunidade n8n para integração com Z-API WhatsApp - Desenvolvido por @icarodev",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "z-api",
    "whatsapp",
    "zapi",
    "whatsapp-api",
    "icarodev"
  ],
  "license": "MIT",
  "author": {
    "name": "IcaroDev",
    "url": "https://github.com/icarodev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icarodev/n8n-nodes-z-api-icarodev"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "lintcheck": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZApiCredentialsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ZApi/ZApi.node.js",
      "dist/nodes/ZApi/ZApiTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}